{
  "expo": {
    "name": "biohacker-mobile",
    "slug": "biohacker-mobile",
    "version": "1.0.0",
    "sdkVersion": "56.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "biohackermobile",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.biohacker.mobile",
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/icon.png"
      },
      "edgeToEdgeEnabled": true,
      "versionCode": 1,
      "targetSdkVersion": 35,
      "minSdkVersion": 24
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "2b992ec0-10ad-4ff6-ab51-64479c4e44de"
      }
    },
    "owner": "wintermute-rooz"
  }
}
